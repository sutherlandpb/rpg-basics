}

// Rotate the particle
rotation += spin_speed;

// Fade the particle
if (fade == 1) {
    alpha -= fading;                    // take away from the alpha value
    if (alpha &lt;= 0) instance_destroy(); // If you cant see the particle, destroy it
}

